+enum avr_device_specific_features
+{
+  AVR_ISA_NONE,
+  AVR_ISA_RMW     = 0x1, /* device has RMW instructions. */
+  AVR_SHORT_SP    = 0x2, /* Stack Pointer has 8 bits width. */
